else
  export NODE_PATH="/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/sanity@3.45.0_@types+node@20.14.2_@types+react@18.3.3_react-dom@18.3.1_react@18.3.1__react@18_3e3jad4bpyddufbuid5gvb6tw4/node_modules/sanity/bin/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/sanity@3.45.0_@types+node@20.14.2_@types+react@18.3.3_react-dom@18.3.1_react@18.3.1__react@18_3e3jad4bpyddufbuid5gvb6tw4/node_modules/sanity/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/sanity@3.45.0_@types+node@20.14.2_@types+react@18.3.3_react-dom@18.3.1_react@18.3.1__react@18_3e3jad4bpyddufbuid5gvb6tw4/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/node_modules:$NODE_PATH"
