#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@sanity+cli@3.45.0_react@18.3.1/node_modules/@sanity/cli/bin/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@sanity+cli@3.45.0_react@18.3.1/node_modules/@sanity/cli/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@sanity+cli@3.45.0_react@18.3.1/node_modules/@sanity/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@sanity+cli@3.45.0_react@18.3.1/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@sanity+cli@3.45.0_react@18.3.1/node_modules/@sanity/cli/bin/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@sanity+cli@3.45.0_react@18.3.1/node_modules/@sanity/cli/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@sanity+cli@3.45.0_react@18.3.1/node_modules/@sanity/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@sanity+cli@3.45.0_react@18.3.1/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/sanity" "$@"
else
  exec node  "$basedir/../../bin/sanity" "$@"
fi
