#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules/@vinxi/listhen/bin/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules/@vinxi/listhen/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules/@vinxi/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules/@vinxi/listhen/bin/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules/@vinxi/listhen/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules/@vinxi/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@vinxi+listhen@1.5.6/node_modules/@vinxi/listhen/bin/listhen.mjs" "$@"
else
  exec node  "$basedir/../../../../../@vinxi+listhen@1.5.6/node_modules/@vinxi/listhen/bin/listhen.mjs" "$@"
fi
