#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/vinxi@0.3.11_@opentelemetry+api@1.9.0_@types+node@20.14.2_ioredis@5.4.1_terser@5.31.1/node_modules/vinxi/bin/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/vinxi@0.3.11_@opentelemetry+api@1.9.0_@types+node@20.14.2_ioredis@5.4.1_terser@5.31.1/node_modules/vinxi/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/vinxi@0.3.11_@opentelemetry+api@1.9.0_@types+node@20.14.2_ioredis@5.4.1_terser@5.31.1/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/vinxi@0.3.11_@opentelemetry+api@1.9.0_@types+node@20.14.2_ioredis@5.4.1_terser@5.31.1/node_modules/vinxi/bin/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/vinxi@0.3.11_@opentelemetry+api@1.9.0_@types+node@20.14.2_ioredis@5.4.1_terser@5.31.1/node_modules/vinxi/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/vinxi@0.3.11_@opentelemetry+api@1.9.0_@types+node@20.14.2_ioredis@5.4.1_terser@5.31.1/node_modules:/Users/tutods/Developer/freelances/impulsionar-solid/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.mjs" "$@"
else
  exec node  "$basedir/../../bin/cli.mjs" "$@"
fi
